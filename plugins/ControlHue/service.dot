<a onclick="sendRequest('{{=it.service_id}}')" href="#" class="item selectable">
    {{=it.name}} YO <span id="{{=it.service_id}}_Status"></span>
</a>

<script>
	
	websocket("{{=it.service_id}}", function(msg){
        element = document.getElementById("{{=it.service_id}}_Status");
        element.innerHtml = "msg";
	});

</script>